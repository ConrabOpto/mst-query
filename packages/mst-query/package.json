    "files": [
        "dist"
    ],
    "devDependencies": {
        "@testing-library/react": "16.3.0",
        "@types/react": "19.1.8",
        "jsdom": "26.1.0",
        "mobx": "6.13.7",
        "mobx-react": "9.2.0",
        "mobx-state-tree": "7.0.2",
        "prettier": "3.6.2",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "tsup": "8.5.0",
        "typescript": "5.8.3",
        "vitest": "3.2.4"
    },
    "peerDependencies": {
        "mobx": ">=6.0.0 <7.0.0",
        "mobx-state-tree": ">=5.0.0 <8.0.0",
